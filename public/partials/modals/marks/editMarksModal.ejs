<!-- EDIT STUDENT MARKS MODAL-->

<div
  class="modal fade"
  id="editMarksModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Edit</h5>
        <button
          class="close"
          type="button"
          data-dismiss="modal"
          aria-label="Close"
        >
          <span aria-hidden="true">Ã—</span>
        </button>
      </div>
      <div class="modal-body">
        <!-- Edit Form Start-->
        <form class="user" action="/edit_marks" method="POST">
          <div class="form-group">
            <div class="form-group col">
              <input
              type="text"
                class="form-control form-control-user"
                placeholder="Course ID"
                name="Course_ID"
                id="Course_ID_Edit"
              />
            </div>
            <div class="form-group col">
              <input
              type="text"
                class="form-control form-control-user"
                placeholder="Register No"
                name="Register_No"
                id="Register_No_Edit"
              />
            </div>
          </div>
          <div class="form-group">
            <div class="form-group col">
              <input
              type="text"
                class="form-control form-control-user"
                placeholder="Category"
                name="Category"
                id="Category_Edit"
              />
            </div>
              <div class="form-group col">
                <input
                  type="number"
                  class="form-control form-control-user"
                  placeholder="Marks"
                  name="Mark"
                  id="Mark_Edit"
                />
            </div>
          </div>

          <hr />
          <input
            type="submit"
            name="Signup"
            value="SUBMIT"
            class="btn btn-primary btn-user btn-block"
          />
        </form>

        <!-- Edit Form End-->
      </div>
    </div>
  </div>
</div>
